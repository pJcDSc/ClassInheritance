#include <iostream>
#include <cstring>
#include "Media.h"

using namespace std;

class Movie : public Media {

 public:

  //Constructor
  Movie(char*, int, char*, int, int);

  //Override getType
  int getType();

  //Get publisher
  char* getPublisher();

  //Get director
  char* getDirector();

  //Get rating
  int getRating();

  //Get duration
  int getDuration();

 private:

  int rating;
  int duration;
  char* publisher;

}
  